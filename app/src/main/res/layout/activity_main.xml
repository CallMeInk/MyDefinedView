<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.example.yukai.mydefinedview.MyView.MyTextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#ff0000"
        android:text="123"/>

    <com.example.yukai.mydefinedview.MyView.MyTopBarView
        android:id="@+id/my_top_bar_view"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        custom:title="MyTobBar"
        custom:titleTextColor="#00ff00"
        custom:titleTextSize="30dp"
        custom:leftText="back"
        custom:leftTextColor="#000000"
        custom:leftBackGround="#ffffff"
        custom:rightText="forward"
        custom:rightTextColor="#000000"
        custom:rightBackground="#ffffff">

    </com.example.yukai.mydefinedview.MyView.MyTopBarView>
</LinearLayout>
