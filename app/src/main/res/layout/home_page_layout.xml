<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_my_defined_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="My Defined View"/>

    <Button
        android:id="@+id/btn_basic_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Basic View"/>

    <Button
        android:id="@+id/btn_art_learning"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Android Art"/>

    <Button
        android:id="@+id/btn_bitmap_cut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="bitmap cut"/>

    <Button
        android:id="@+id/btn_external_view_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="external view test"/>

    <Button
        android:id="@+id/btn_list_view_anim_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="work test"/>

    <Button
        android:id="@+id/btn_example_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="btn example test"/>

</LinearLayout>